<?import javafx.scene.control.PasswordField?>
<Pane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="account.AccountCreation">
   <fx:include source="toolbar.fxml"/>
   <Label layoutX="53.0" layoutY="86.0" text="Account Creation">
      <font>
         <Font name="Arial Bold" size="18.0"/>
      </font>
   </Label>
   <Label layoutX="102.0" layoutY="132.0" text="Username"/>
   <TextField fx:id="txtUsername" layoutX="55.0" layoutY="149.0"/>
   <Label layoutX="104.0" layoutY="192.0" text="Password"/>
   <PasswordField fx:id="txtPassword" layoutX="55.0" layoutY="209.0"/>
   <Label layoutX="81.0" layoutY="257.0" text="Confirm Password"/>
   <PasswordField fx:id="txtConfirmPassword" layoutX="55.0" layoutY="274.0"/>
   <Button fx:id="btnRegister" layoutX="359.0" layoutY="344.0" mnemonicParsing="false" onAction="#onAccRegister"
           text="Create Account"/>
   <Label layoutX="364.0" layoutY="192.0" text="Name"/>
   <TextField fx:id="txtName" layoutX="305.0" layoutY="209.0"/>
   <Label layoutX="352.0" layoutY="257.0" text="Last Name"/>
   <TextField fx:id="txtLastName" layoutX="305.0" layoutY="274.0"/>
   <Button fx:id="btnTest" layoutX="127.0" layoutY="357.0" mnemonicParsing="false" onAction="#testSession"
           text="TestBtn"/>
</Pane>
